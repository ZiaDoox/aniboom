sortMethod?' component={CategoryScreen} exact/>\r\n          <Route path='/products/:category/page/:pageNumber/:sortMethod?' component={CategoryScreen} exact />\r\n\r\n          <Route\r\n            path='/admin/productlist'\r\n            component={ProductListScreen}\r\n            exact\r\n          />\r\n          <Route\r\n            path='/admin/productlist/:pageNumber'\r\n            component={ProductListScreen}\r\n            exact\r\n          />\r\n          <Route path='/admin/product/:id/edit' component={ProductEditScreen} />\r\n          <Route path='/admin/orderlist' component={OrderListScreen} />\r\n          <Route path='/search/:keyword/:sortMethod?' component={Search} exact />\r\n          <Route\r\n            path='/search/:keyword/page/:pageNumber/:sortMethod?'\r\n            component={Search}\r\n            exact\r\n          />\r\n          <Route path='/' component={HomeScreen} exact />\r\n      </main>\r\n      <Footer />\r\n    </Router>\r\n  )\r\n}\r\n\r\nexport default App\r\n","C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\serviceWorker.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\reducers\\orderReducers.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\reducers\\userReducers.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\screens\\ServiceScreen.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\screens\\LoginScreen.js",[],"C:\\Users\\ziadp\\OneDrive\\Documents\\_WORK\\aniboom\\frontend\\src\\screens\\ProductScreen.js",["238"],"import React, { useState, useEffect } from 'react'\r\nimport { Link } from 'react-router-dom'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport { Row,